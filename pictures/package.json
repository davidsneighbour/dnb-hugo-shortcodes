{
  "name": "@dnb-hugo-shortcodes/pictures",
  "description": "Image shortcodes for Hugo",
  "version": "0.0.9",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/dnb-hugo/shortcodes/issues"
  },
  "devDependencies": {
    "@dnb-hugo/remark-config": "3.0.1",
    "@dnb-hugo/standard-version-config": "3.0.1"
  },
  "homepage": "https://github.com/dnb-hugo/shortcodes",
  "license": "MIT",
  "private": true,
  "scripts": {
    "release": "standard-version --release-as patch -a -t \"youtube/v\" --releaseCommitMessageFormat \"chore(release): youtube/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-major": "standard-version --release-as major -a -t \"youtube/v\" --releaseCommitMessageFormat \"chore(release): youtube/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "release-next": "standard-version --release-as minor -a -t \"youtube/v\" --releaseCommitMessageFormat \"chore(release): youtube/v{{currentTag}}\" && ./bin/release-hook-postrelease.sh",
    "remark": "remark"
  }
}
